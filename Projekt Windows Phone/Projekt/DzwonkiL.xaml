            <TextBlock Text="AMW Helper - Dzwonki" Style="{StaticResource PhoneTextNormalStyle}"/>
        </StackPanel>

        <!--ContentPanel - place additional content here-->
        <Grid x:Name="ContentPanel" Margin="0,49,24,10" Grid.RowSpan="2">
